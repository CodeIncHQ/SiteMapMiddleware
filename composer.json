{
  "name": "codeinc/sitemap-middlware",
  "version": "1.0.0",
  "description": "A PSR-15 middleware to answer /sitemap.xml requests",
  "homepage": "https://github.com/CodeIncHQ/SiteMapMiddleware",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0",
    "tackk/cartographer": "^1.1",
    "guzzlehttp/psr7": "^1.4",
    "codeinc/psr7-responses": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "codeinc/psr7-response-sender": "^1.0"
  },
  "autoload": {
    "psr-4": {"CodeInc\\SiteMapMiddleware\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "CodeInc\\SiteMapMiddleware\\Tests\\": "tests/" }
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "suggest": {
    "codeinc/middleware-dispatcher": "A PSR-15 middleware dispatcher",
    "codeinc/psr7-response-sender": "A PSR-7 response sender"
  }
}
